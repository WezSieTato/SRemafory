# platform :ios, '6.0'

source 'https://github.com/CocoaPods/Specs.git'
inhibit_all_warnings!

target 'SRemafory' do
    pod 'ProtocolBuffers', '~> 1.9'
    pod "objc-zmq", :git => 'https://github.com/jeremy-w/objc-zmq.git'
end

target 'SRemaforyClient' do
    pod 'ProtocolBuffers', '~> 1.9'
    pod "objc-zmq", :git => 'https://github.com/jeremy-w/objc-zmq.git'
end